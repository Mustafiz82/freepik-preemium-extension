{
  "manifest_version": 3,
  "name": "Freepic Premium",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "scripting" , "cookies"],
  "host_permissions": [
    "https://script.google.com/*",
    "https://www.freepik.com/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup" : "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Freepic Premium"
  },
  "content_scripts": [
    {
      "matches": ["https://www.freepik.com/*"],
      "js": ["content.js"]
    }
  ]
}
